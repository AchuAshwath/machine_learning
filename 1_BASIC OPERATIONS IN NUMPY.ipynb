   "source": [
    "import numpy as np\n",
    "\n",
    "a =    np.arange(0,9).reshape(3,3)\n",
    "print(\"3x3 matrix \\n\",a)\n",
    "b = a[::2,1::2]\n",
    "print(\"odd and even numbers\\n \",b)\n",
    "c = 3*a\n",
    "print(\"Multiplying\\n\",c)\n",
    "d = np.identity((3),dtype = int)\n",
    "print(\"Adding with identity matrix\\n\",c+d)"
